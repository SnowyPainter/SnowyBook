@using SnowyBook.Models
@model LatelyNotesModel

<div class="container">
    <h1>Revised documents in these days</h1>
    <ol>
        @for(int i = 0;i < Model.Notes.Count();i++)
        {
            var title = @Model.Notes[i].Title;
            <li>
                <a href="/Home/Edit/@title/Chief">@title</a>
            </li>
        }
    </ol>
</div>